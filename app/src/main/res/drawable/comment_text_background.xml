<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>

            <!-- Viewの枠線を設定 -->
            <stroke
                android:width="2dp"
                android:color="@color/brown_dark"
                android:dashWidth="5dp"
                android:dashGap="2dp"/>
            <!-- Viewの角丸を設定 -->
            <corners android:radius="5dp" />
        </shape>
    </item>

</selector>